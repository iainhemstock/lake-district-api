{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://192.168.0.16:8080/schemas",
	"type": "object",
	"properties": {
		"classifications": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"height": {
			"type": "object",
			"properties": {
				"feet": {
					"type": "string"
				},
				"meters": {
					"type": "string"
				}
			},
			"required": ["feet", "meters"],
			"additionalProperties": false
		},
		"location": {
			"type": "object",
			"properties": {
				"latitude": {
					"type": "string"
				},
				"longitude": {
					"type": "string"
				},
				"dms": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"degrees": {
								"type": "string"
							},
							"minutes": {
								"type": "string"
							},
							"seconds": {
								"type": "string"
							},
							"hemisphere": {
								"type": "string"
							}
						},
						"required": ["degrees", "minutes", "seconds", "hemisphere"],
						"additionalProperties": false
					},
					"minItems": 2,
					"maxItems": 2
				},
				"region": {
					"type": "string"
				},
				"os_map_ref": {
					"type": "string"
				},
				"os_maps": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			},
			"required": ["latitude", "longitude", "dms", "region", "os_map_ref", "os_maps"],
			"additionalProperties": false
		},
		"name": {
			"type": "string"
		},
		"parent_peak": {
			"type": "string"
		},
		"prominence": {
			"type": "object",
			"properties": {
				"feet": {
					"type": "string"
				},
				"meters": {
					"type": "string"
				}
			},
			"required": ["feet", "meters"],
			"additionalProperties": false
		},
		"url": {
			"type": "string"
		}
	},
	"required": ["classifications", "height", "location", "name", "parent_peak", "prominence", "url"],
	"additionalProperties": false
}