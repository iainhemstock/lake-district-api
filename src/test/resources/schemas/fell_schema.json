{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://192.168.0.16:8080/schemas",
	"type": "object",
	"properties": {
		"classifications": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"height": {
			"type": "object",
			"properties": {
				"feet": {
					"type": "integer",
					"minimum": 0
				},
				"meters": {
					"type": "integer",
					"minimum": 0
				}
			},
			"required": ["feet", "meters"]
		},
		"location": {
			"type": "object",
			"properties": {
				"coords": {
					"type": "object",
					"properties": {
						"decimal": {
							"type": "object",
							"properties": {
								"latitude": {
									"type": "string"
								},
								"longitude": {
									"type": "string"
								}
							},
							"required": ["latitude", "longitude"]
						},
						"dms": {
							"type": "object",
							"properties": {
								"y": {
									"type": "object",
									"properties": {
										"degrees": {
											"type": "integer"
										},
										"minutes": {
											"type": "integer"
										},
										"seconds": {
											"type": "integer"
										},
										"hemisphere": {
											"type": "string"
										},
										"formatted": {
											"type": "string"
										}
									},
									"required": ["degrees", "minutes", "seconds", "hemisphere", "formatted"]
								},
								"x": {
									"type": "object",
									"properties": {
										"degrees": {
											"type": "integer"
										},
										"minutes": {
											"type": "integer"
										},
										"seconds": {
											"type": "integer"
										},
										"hemisphere": {
											"type": "string"
										},
										"formatted": {
											"type": "string"
										}
									},
									"required": ["degrees", "minutes", "seconds", "hemisphere", "formatted"]
								}
							},
							"required": ["y", "x"]
						}
					},
					"required": ["decimal", "dms"]
				},
				"region": {
					"type": "string"
				},
				"os_map_ref": {
					"type": "string"
				},
				"os_maps": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			},
			"required": ["coords", "region", "os_map_ref", "os_maps"]
		},
		"name": {
			"type": "string"
		},
		"parent_peak": {
			"type": "string"
		},
		"prominence": {
			"type": "object",
			"properties": {
				"feet": {
					"type": "integer",
					"minimum": 0
				},
				"meters": {
					"type": "integer",
					"minimum": 0
				}
			},
			"required": ["feet", "meters"]
		},
		"url": {
			"type": "string"
		}
	},
	"required": ["classifications", "height", "location", "name", "parent_peak", "prominence", "url"],
	"additionalProperties": false
}