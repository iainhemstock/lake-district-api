{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://192.168.0.16:8080/schemas",
	"type": "object",
	"required": ["links", "classifications", "height", "location", "name", "prominence"],
	"additionalProperties": false,
	"properties": {
		"links": {
			"type": "object",
			"required": ["self"],
			"additionalProperties": true,
			"properties": {
				"self": {
					"type": "object",
					"required": ["href"],
					"additionalProperties": false,
					"properties": {
						"href": {
							"type": "string"
						}
					}
				},
				"parent": {
					"type": "object",
					"required": ["href"],
					"additionalProperties": false,
					"properties": {
						"href": {
							"type": "string"
						}
					}
				}
			}
		},
		"classifications": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"height": {
			"type": "object",
			"required": ["feet", "meters"],
			"additionalProperties": false,
			"properties": {
				"feet": {
					"type": "string"
				},
				"meters": {
					"type": "string"
				}
			}
		},
		"location": {
			"type": "object",
			"required": ["latitude", "longitude", "dms_latitude", "dms_longitude", "region", "os_map_ref", "os_maps"],
			"additionalProperties": false,
			"properties": {
				"latitude": {
					"type": "string"
				},
				"longitude": {
					"type": "string"
				},
				"dms_latitude": {
					"type": "object",
					"required": ["degrees", "minutes", "seconds", "hemisphere"],
					"additionalProperties": false,
					"properties": {
						"degrees": {
							"type": "string"
						},
						"minutes": {
							"type": "string"
						},
						"seconds": {
							"type": "string"
						},
						"hemisphere": {
							"type": "string"
						}
					}
				},
				"dms_longitude": {
					"type": "object",
					"required": ["degrees", "minutes", "seconds", "hemisphere"],
					"additionalProperties": false,
					"properties": {
						"degrees": {
							"type": "string"
						},
						"minutes": {
							"type": "string"
						},
						"seconds": {
							"type": "string"
						},
						"hemisphere": {
							"type": "string"
						}
					}
				},
				"region": {
					"type": "string"
				},
				"os_map_ref": {
					"type": "string"
				},
				"os_maps": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			}
		},
		"name": {
			"type": "string"
		},
		"prominence": {
			"type": "object",
			"required": ["feet", "meters"],
			"additionalProperties": false,
			"properties": {
				"feet": {
					"type": "string"
				},
				"meters": {
					"type": "string"
				}
			}
		}
	}
}